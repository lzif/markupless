{
  "name": "@px/void",
  "version": "0.1.0",
  "description": "fluent, readable JavaScript framework that eliminates markup with magical abstractions.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "keywords": [
    "frontend",
    "framework",
    "typescript",
    "ui",
    "reactive",
    "lightweight"
  ],
  "author": "Luki Zainur <lukyfriendly@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lzif/markupless.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "typescript": "^5.7.2",
    "vite": "^6.0.2",
    "vitest": "^2.1.8",
    "@antfu/eslint-config": "^3.11.2",
    "eslint": "^9.15.0"
  },
  "vitest": {
    "environment": "jsdom",
    "globals": true,
    "setupFiles": [
      "./vitest.setup.ts"
    ]
  }
}
